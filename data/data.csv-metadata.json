{
  "@context": "http://www.w3.org/ns/csvw",
  "url": "data.csv",
  "schema:description": "Bulk Import CSV file for MicroAuthority.",
  "schema:creator": {
    "schema:name": "David Newbury",
    "schema:url": "http://www.workergnome.com"
  },
  "aboutUrl": "{+domain}/{id}",
  "tableSchema": {
    "primaryKey": "id",
    "columns": [
      {
        "titles": "id",
        "name": "id",
        "datatype": "string",
        "required": true,
        "suppressOutput": true
      },
      {
        "titles": "name",
        "propertyUrl": "schema:name",
        "datatype": "string",
        "required": true
      },
      {
        "titles": "301_redirect",
        "schema:description": "This should a URL for a permanent redirect for this entity.  A value in this field will indicate that any URI generated by MicroAuthority has been permanently superseded by the URI provided in this field.",
        "datatype": "anyURI"
      },
      {
        "titles": "permalink",
        "schema:description": "This should a URL for a preferred HTML page for this entity.  A value in this field will redirect HTML (aka human-readable) requests to this URL, but will continue to serve structured data.  (If there is a 301 redirect, that takes priority over this value.",
        "datatype": "anyURI"
      },
      {
        "titles": "entityType",
        "datatype": {
          "dc:title": "Schema.org Base Type",
          "dc:description": "Either a schema:Thing, or one if its direct subclasses",
          "base": "string",
          "format": "Thing|Action|CreativeWork|Event|Intangible|Organization|Person|Place|Product"
        },
        "required": true,
        "valueUrl": "schema:{entityType}",
        "propertyUrl": "rdf:type"

      },
      {
        "titles": "givenName",
        "propertyUrl": "schema:givenName"
      },
      {
        "titles": "familyName",
        "propertyUrl": "schema:familyName"
      },
      {
        "titles": "sortName",
        "propertyUrl": "schema:alternateName"
      },
      {
        "titles": "alternateName",
        "separator": "|"
      },
      {"titles": "gender",
        "separator": "|",
        "propertyUrl": "schema:gender"
      },
      {
        "titles": "birthDate",
        "datatype": "date",
        "propertyUrl": "schema:birthDate"
      },
      {
        "titles": "birthPlace",
        "aboutUrl": "{+domain}/{id}/birthplace",
        "propertyUrl": "schema:name"
      },
      {
        "titles": "deathDate",
        "datatype": "date",
        "propertyUrl": "schema:deathDate"
      },
      {
        "titles": "deathPlace",
        "aboutUrl": "{+domain}/{id}/deathplace",
        "propertyUrl": "schema:name"
      },
      {
        "titles": "webpage",
        "datatype": "anyURI",
        "propertyUrl": "schema:mainEntityOfPage"
      },
      {
        "titles": "relatedWebpage",
        "datatype": "anyURI",
        "separator": "|",
        "suppressOutput": true
      },
      {
        "titles": "sameAs",
        "datatype": "anyURI",
        "separator": "|",
        "propertyUrl": "schema:sameAs"
      },
      {
        "titles": "image",
        "datatype": "anyURI",
        "separator": "|",
        "propertyUrl": "schema:image"
      },
      {
        "virtual": true,
        "propertyUrl": "rdf:type",
        "aboutUrl": "{+domain}/{id}/birthplace",
        "valueUrl": "schema:Place"
      },
      {
        "virtual": true,
        "propertyUrl": "schema:birthPlace",
        "valueUrl": "{+domain}/{id}/birthplace"
      },
      {
        "virtual": true,
        "propertyUrl": "rdf:type",
        "aboutUrl": "{+domain}/{id}/deathplace",
        "valueUrl": "schema:Place"
      },
      {
        "virtual": true,
        "propertyUrl": "schema:deathPlace",
        "valueUrl": "{+domain}/{id}/deathplace"
      },
      {
        "virtual": true,
        "name": "domain",
        "default": "http://microauthority.museumprovenance.org",
        "suppressOutput": true
      }
    ]
  }
}

